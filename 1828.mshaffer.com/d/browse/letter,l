	// store from URL, then from cookie if not found!
	myArr = getSearch();

	if(isArray(myArr))
		{
		var tKeywords = myArr[1].split("+").join(" ");
			var tKeywordsA = tKeywords.split("?");
			document.search_1828.keywords.value = tKeywordsA[0];
			
		for(i=0; i<document.search_1828.category.length;i++)
			{
			if (myArr[0]==document.search_1828.category[i].value)
				{
				document.search_1828.category[i].selected = true;
				break;
				}
			}
			// if from plugin
			if(tKeywords[1] != undefined){storeCookie(document.search_1828.category[i].value,document.search_1828.keywords.value);}
		


		}
	else
		{
		// from cookie

		var c_search_1828 = new CJL_CookieUtil("session_cookie",null,"/");

