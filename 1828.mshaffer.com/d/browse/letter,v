<SCRIPT>
	function storeCookie(category,keywords)
		{
		var c_search_1828 = new CJL_CookieUtil("session_cookie",null,"/");
			c_search_1828.setSubValue("category", category);
			c_search_1828.setSubValue("keywords", keywords);

		}
	function searchPlugin()
		{

		}
	function submit1828Search()
		{
		document.search_1828.submitMe.disabled=true;

		category = document.search_1828.category[document.search_1828.category.selectedIndex].value;

		keywords = document.search_1828.keywords.value;

		if(!keywords){keywords = "sample";}



		// set Cookie
		storeCookie(category,keywords);



		


		// build URL

		
		keywords = keywords.split(" ").join("+");

		myURL = "/d/search/"+category+","+keywords;

		window.location = (myURL);

		return false;

		}
</SCRIPT>
<FORM name="search_1828" onSubmit="return submit1828Search();">
